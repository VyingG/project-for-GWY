body
	background-color #EFEFF4

.headnav
	width 100%
	height 50px
	background-color white
	border-bottom 1px solid #D6D7DC
	position relative

.headnav .visitlogin
	line-height 50px
	display block
	color gray
	position absolute
	left 5%

.headnav>.navname
	display block
	text-align center
	line-height 50px
	font-size 18px
	color #ED6F36

.itembox
	width 100%
	height 103px
	margin-top 20px
	background-color white
	border-top 1px solid #ECECF0
	border-bottom 1px solid #ECECF0

.itembox>.iptline
	width 90%
	height 50px
	margin 0 auto
	position relative

.iptline>.itemicon
	width 20px
	height 20px
	display block
	float left
	margin-top 15px
	position absolute
	left 0

.iptline>.itemname
	line-height 50px
	margin-left 30px
	display inline-block

.iptline>.putin
	font-size 16px
	width 70%
	height 30px
	border none
	outline none

.itembox>.splitline
	width 90%
	height 1px
	margin 0 auto
	background-color #ECECF0

.subbtn
	width 90%
	height 45px
	background-color #C7C7C7
	border none
	outline none
	font-size 16px
	color white
	margin 0 auto
	margin-top 20px
	border-radius 5px
	display block

.othergo
	width 90%
	height auto
	margin 0 auto
	margin-top 10px

.othergo>.register
	float left
	color #FF8A00

.othergo>.forgetpsw
	float right
	color black

.remind
	display block
	width 120px
	height 24px
	background-color #696969
	border-radius 15px
	text-align center
	line-height 24px
	font-size 14px
	color white
	margin 10px auto
	display none